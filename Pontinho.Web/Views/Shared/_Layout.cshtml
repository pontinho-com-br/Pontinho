<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <base href="/" />
    <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
    <link rel="stylesheet" href="@Url.Content("~/Content/bootstrap/css/bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/fonts/material-design-iconic-font/css/material-design-iconic-font.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/jquery-ui.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/select2.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/font-awesome-4.4.0/css/font-awesome.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/fontello.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/chartist.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/bootstrap-datepicker.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/animate.css")">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.1/moment.min.js"></script>
    <script src="http://code.jquery.com/jquery-3.1.1.min.js"></script>
    <script src="http://code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.1.0/fullcalendar.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/modules/data.js"></script>
    <script src="https://code.highcharts.com/modules/drilldown.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/hammer.js/2.0.8/hammer.min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/RyanMullins-angular-hammer/2.2.0/angular.hammer.min.js"></script>*@
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet" />
    <!-- Google fonts -->
    <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="@Url.Content("~/Content/Site.css")">
</head>
<body>

    @RenderBody()

    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Content/js/jquery-ui.min.js")"></script>
    <script src="@Url.Content("~/Content/js/select2.min.js")"></script>
    <script src="@Url.Content("~/Content/js/parsley.min.js")"></script>
    <script src="@Url.Content("~/Content/js/throttle-debounce.min.js")"></script>
    <script src="@Url.Content("~/Content/js/jquery.shuffle.min.js")"></script>
    <script src="@Url.Content("~/Content/js/autosize.min.js")"></script>
    <script src="@Url.Content("~/Content/js/raphael-min.js")"></script>
    <script src="@Url.Content("~/Content/js/morris.min.js")"></script>
    <script src="@Url.Content("~/Content/js/Chart.min.js")"></script>
    <script src="@Url.Content("~/Content/js/chartist.min.js")"></script>
    <script src="@Url.Content("~/Content/bootstrap/js/bootstrap.min.js")"></script>
    @*<script src="@Url.Content("~/Content/js/ui-bootstrap-tpls-1.3.3.min.js")"></script>*@
    <div class="visible-xs visible-sm extendedChecker"></div>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
</body>
</html>
